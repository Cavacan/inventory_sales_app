<h1>ログイン</h1>

<%= form_with url: admin_login_path, local: true do |f| %>
  <div>
    <%= f.label :email %>
    <%= f.email_field :email %>
  </div>
  <div>
    <%= f.label :password %>
    <%= f.text_field :password %>
  </div>
  <div>
    <%= f.submit "ログイン" %>
  </div>
<% end %>

<br>
<nav>
  <%= link_to 'トップページへ戻る', root_path %>
</nav>

<br>

